name: QX2Stash
desc: QX重写转换Stash（+"qx.stoverride"）

http:
  script:
    - match: qx.stoverride$
      name: QX2Stash
      type: request
      require-body: true
      timeout: 30
      max-size: 0
  mitm:
    - "github.com"
    - "raw.githubusercontent.com"

script-providers:
  QX2Stash:
    url: https://raw.githubusercontent.com/chengkongyiban/stash/main/js/QX_to_Stash.js
    interval: 86400
